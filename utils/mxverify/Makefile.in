SHELL=		/bin/sh
TOPDIR=		../..
PZCONFIG=	$(TOPDIR)/zmailer.Config
INSTALL=	@INSTALL@
MAILBIN=	$(prefix)@MAILBIN@

srcdir=         @srcdir@
VPATH=          @srcdir@

CC=		@CC@
COPTS=		@CFLAGS@
CPPFLAGS=	@CPPFLAGS@
CPPDEP=		@CPPDEP@
DEFS=		@DEFS@
MD5SUM=		@MD5SUM@
#
INCL=		-I$(srcdir)/$(TOPDIR)/include -I$(TOPDIR)/include -I$(TOPDIR)  -I.
CFLAGS=		$(COPTS) $(CPPFLAGS) $(DEFS) $(INCL)
LIBMALLOC=	@LIBMALLOC@
LIBMALLOCDEB=	$(TOPDIR)/libmalloc/libmalloc_d.a-a
#
LIB=		@LIBRESOLV@ @LIBSOCKET@
LIBDEB=		$(TOPDIR)/libs/libtag

OBJS=		mxverify-cgi.o gai_strerror.o getaddrinfo.o inet_ntop.o \
		myhostname.o

BINS=	

all:	mxverify-cgi

install:

.c.o:
	$(CC) $(CFLAGS) -c $<

mxverify-cgi: $(OBJS)
	$(CC) $(CFLAGS) -o $@.x $(OBJS) $(LIB)
	mv $@.x $@

FRC:

clean:	FRC
	rm -f ./+* *~ *.o mxverify-cgi

distclean: clean
	rm -f Makefile

depend:
