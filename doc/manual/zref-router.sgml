<VARIABLELIST>
<VARLISTENTRY><TERM>Syntax:</TERM><LISTITEM><PARA>

{\tt recase [-u \verb/|/ -l \verb/|/ -p] string}

</PARA></LISTITEM></VARLISTENTRY>
<VARLISTENTRY><TERM>Return Values:</TERM><LISTITEM><PARA>

<VARIABLELIST>
<VARLISTENTRY><TERM>{\tt 0}</TERM><LISTITEM><PARA>



</PARA></LISTITEM></VARLISTENTRY>
